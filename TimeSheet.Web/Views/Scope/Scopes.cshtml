@model ScopeModel

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Scopes[0].Id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Scopes[0].Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Scopes[0].TotalPrice)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Scopes[0].NameCurrency)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Scopes[0].TotalPriceUSD)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Scopes[0].TotalPriceInBTC)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.Scopes) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TotalPrice)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NameCurrency)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TotalPriceUSD)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TotalPriceInBTC)
            </td>
        </tr>
}
    </tbody>
</table>

@await Html.PartialAsync("~/Views/MemoryCache/MemoryCacheData.cshtml", Model.CacheDateTime)