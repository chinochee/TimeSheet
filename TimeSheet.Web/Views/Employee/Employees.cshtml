@model EmployeeModel

<table class="table">
    <thead>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Employees[0].Id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Employees[0].Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Employees[0].TotalPriceUSD)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Employees[0].TotalPriceInBTC)
        </th>
        <th></th>
    </tr>
    </thead>
    <tbody>
    @foreach (var item in Model.Employees) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TotalPriceUSD)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TotalPriceInBTC)
            </td>
        </tr>
    }
    </tbody>
</table>

@await Html.PartialAsync("~/Views/MemoryCache/MemoryCacheData.cshtml", Model.CacheDateTime)